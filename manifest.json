{
  "name": "Calculate",
  "description": "A Simple Offline Calculator that works in a Popup.",
  "version": "0.1.9",
  "manifest_version" : 2,
  "app": {
    "launch": {
      "local_path": "calc.html",
      "container": "panel",
      "height" : 360,
      "width" : 280
    }
  },
  "icons": {
    "16" : "icon_16.png",
    "48" : "icon_48.png",
    "128" : "icon_128.png",
    "256" : "icon_256.png"
  },
  "options_page" : "settings.html",
  "offline_enabled": true
}